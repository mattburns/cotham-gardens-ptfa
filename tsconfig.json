{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@content/*": [
        "./src/content/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ]
    }
  }
}